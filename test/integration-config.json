{
  "HUB_PARTICIPANT": {
    "ID": 1,
    "NAME": "Hub"
  },
  "TEST_ALS_HOST":"account-lookup-service",
  "ADMIN_PORT": 4001,
  "API_PORT": 4002,
  "DATABASE": {
    "DIALECT": "mysql",
    "HOST": "mysql-als",
    "PORT": 3306,
    "USER": "account_lookup",
    "PASSWORD": "password",
    "DATABASE": "account_lookup",
    "POOL_MIN_SIZE": 10,
    "POOL_MAX_SIZE": 10,
    "ACQUIRE_TIMEOUT_MILLIS": 30000,
    "CREATE_TIMEOUT_MILLIS": 30000,
    "DESTROY_TIMEOUT_MILLIS": 5000,
    "IDLE_TIMEOUT_MILLIS": 30000,
    "REAP_INTERVAL_MILLIS": 1000,
    "CREATE_RETRY_INTERVAL_MILLIS": 200,
    "DEBUG": false
  },
  "PROTOCOL_VERSIONS": {
    "CONTENT": {
      "DEFAULT": "1.1",
      "VALIDATELIST": [
        "1.1",
        "1.0"
      ]
    },
    "ACCEPT": {
      "DEFAULT": "1",
      "VALIDATELIST": [
        "1",
        "1.0",
        "1.1"
      ]
    }
  },
  "DISPLAY_ROUTES": true,
  "RUN_MIGRATIONS": true,
  "CENTRAL_SHARED_ENDPOINT_CACHE_CONFIG": {
    "expiresIn": 180000,
    "generateTimeout": 30000
  },
  "PROXY_CACHE": {
    "enabled": true,
    "type": "redis-cluster",
    "proxyConfig": {
      "cluster": [
        { "host": "redis-node-0", "port": 6379 }
      ]
    }
  },
  "SWITCH_ENDPOINT": "http://localhost:3001",
  "INSTRUMENTATION": {
    "METRICS": {
      "DISABLED": false,
      "labels": {
        "fspId": "*"
      },
      "config": {
        "timeout": 5000,
        "prefix": "moja_",
        "defaultLabels": {
          "serviceName": "account-lookup-service"
        }
      }
    }
  },
  "ENDPOINT_SECURITY":{
    "JWS": {
      "JWS_SIGN": false,
      "JWS_SIGNING_KEY_PATH": "secrets/jwsSigningKey.key"
    }
  },
  "API_DOC_ENDPOINTS_ENABLED": true
}
