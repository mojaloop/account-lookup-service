      "DISABLED": false,
      "labels": {
        "fspId": "*"
      },
      "config": {
        "timeout": 5000,
        "prefix": "moja_",
        "defaultLabels": {
          "serviceName": "account-lookup-service"
        }
      }
    }
  }
}
